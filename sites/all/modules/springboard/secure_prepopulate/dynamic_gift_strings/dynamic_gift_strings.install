/**
 * @file
 *   Dynamic gift strings install/schema hooks
 */

/**
 * Implements hook_uninstall().
 */
function dynamic_gift_strings_uninstall() {
  variable_del('dynamic_gift_strings_send_alert');
  variable_del('dynamic_gift_strings_alert_email');
  variable_del('dynamic_gift_strings_never_expire');
}
